{
  "name": "nuxt-headless",
  "version": "1.1.0",
  "description": "Nuxt Headless",
  "author": "Alessandro Giordo - <16958684+bovas85@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt --modern",
    "build": "cross-env NODE_ENV=production nuxt build --modern",
    "start": "cross-env NODE_ENV=production nuxt-start --modern",
    "startProd": "cross-env NODE_ENV=production nuxt-start --modern",
    "generate": "nuxt generate --modern",
    "staging": "cross-env NODE_ENV=development now --public",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead",
    "not ie > 0"
  ],
  "greenkeeper": {
    "ignore": [
      "scrollama",
      "vue-awesome-swiper"
    ]
  },
  "dependencies": {
    "@nuxt/http": "^0.3.2",
    "@nuxtjs/component-cache": "^1.1.5",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/sitemap": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "bulma": "^0.7.5",
    "cookie-universal-nuxt": "^2.0.18",
    "intersection-observer": "^0.7.0",
    "lazysizes": "^5.1.2",
    "nuxt": "^2.10.0",
    "scrollama": "1.4.4",
    "splitting": "^1.0.6",
    "vue-awesome-swiper": "2.6.7",
    "vue-intersect": "^1.1.3",
    "vue-localstorage": "^0.6.2",
    "vue-media": "^1.1.1",
    "vue-scrollto": "^2.17.1",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-vue": "^5.2.3",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "nuxt-purgecss": "^0.2.1",
    "nuxt-start": "^2.10.0",
    "open": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-responsive-type": "^1.0.0",
    "sass-loader": "^8.0.0"
  }
}
